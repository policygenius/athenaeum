{
  "name": "athenaeum",
  "version": "7.12.0",
  "description": "PolicyGenius React Component Library (RCL)",
  "main": "lib/index.js",
  "scripts": {
    "cc": "yarn plop create-component",
    "reset": "rm -rf node_modules && yarn",
    "open": "open http://localhost:6060",
    "prepush": "if [ \"$ENABLE_COMMIT_HOOKS\" == 'true' ]; then yarn lint && yarn test; fi",
    "precommit": "if [ \"$ENABLE_COMMIT_HOOKS\" == 'true' ]; then bin/check_code && bin/todo_list; fi",
    "start": "yarn dev",
    "dev": "yarn styleguide:build && yarn run styleguide",
    "stylin": "yarn install && yarn start & yarn run open",
    "nav": "yarn run clean:nav && NODE_ENV=development webpack-dev-server --config ./webpack.config.static.styleguide.js --hot --inline --env.static=main_nav",
    "footer": "yarn run clean:footer && NODE_ENV=development webpack-dev-server --config ./webpack.config.static.styleguide.js --hot --inline --env.static=footer",
    "styleguide": "styleguidist server",
    "styleguide:build": "rm -rf ./styleguide/* && styleguidist build",
    "build:publish": "NODE_ENV='production' ./node_modules/webpack/bin/webpack.js --config ./webpack.config.publish.js --progress --colors",
    "build:publish:watch": "NODE_ENV='production' ./node_modules/webpack/bin/webpack.js --config ./webpack.config.publish.js --progress --colors --watch",
    "build:publish:nav": "yarn clean:nav && yarn build:nav:bundle && yarn build:nav:static && yarn build:nav:html",
    "build:publish:footer": "yarn clean:footer && yarn build:footer:bundle && yarn build:footer:static && yarn build:footer:html",
    "publish": "rm -rf ./lib/* && yarn build:publish && npm publish",
    "clean:nav": "rm -rf ./main_nav/build/* && rm -rf ./main_nav/static/*",
    "clean:footer": "rm -rf ./footer/build/* && rm -rf ./footer/static/*",
    "build:nav:styleguide": "./node_modules/webpack/bin/webpack.js --config ./webpack.config.static.styleguide.js --progress --colors --env.static=main_nav",
    "build:nav:bundle": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config ./webpack.config.static.bundle.js --progress --colors --env.static=main_nav",
    "build:nav:static": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config ./webpack.config.static.server.js --progress --colors --env.static=main_nav",
    "build:nav:html": "node main_nav/index.js",
    "build:footer:styleguide": "./node_modules/webpack/bin/webpack.js --config ./webpack.config.static.styleguide.js --progress --colors --env.static=footer",
    "build:footer:bundle": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config ./webpack.config.static.bundle.js --progress --colors --env.static=footer",
    "build:footer:static": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config ./webpack.config.static.server.js --progress --colors --env.static=footer",
    "build:footer:html": "node footer/index.js",
    "test": "NODE_ENV='test' jest",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:md",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- ./src ",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:md": "eslint --ext md .",
    "snapshot": "yarn test -- --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/policygenius/athenaeum.git"
  },
  "author": "PolicyGenius",
  "license": "MIT",
  "homepage": "https://github.com/policygenius/athenaeum#readme",
  "dependencies": {
    "accounting": "^0.4.1",
    "babel-preset-env": "^1.6.0",
    "classnames": "^2.2.5",
    "cloudinary-react": "^1.0.4",
    "critical": "^0.9.0",
    "express": "^4.16.2",
    "holderjs": "^2.9.4",
    "html-tag-names": "^1.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "lazysizes": "^4.0.1",
    "lodash": "^4.17.4",
    "npm": "^5.0.0",
    "react-input-mask": "^0.8.0",
    "react-modal": "^3.1.6",
    "react-stickynode": "^1.4.0",
    "react-styleguidist": "^6.0.26",
    "react-svg-inline": "^2.0.0",
    "shortid": "^2.2.8",
    "sinon": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-markdown": "https://github.com/jmcolella/eslint-plugin-markdown/tarball/master",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.13.3",
    "image-webpack-loader": "^3.3.0",
    "imagemin-webpack-plugin": "^1.5.0-beta.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-verbose-reporter": "0.0.3",
    "karma-webpack": "^1.8.0",
    "klaw-sync": "^2.1.0",
    "markdownlint-cli": "^0.3.1",
    "mocha": "^3.1.2",
    "node-sass": "^4.5.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "plop": "^1.7.4",
    "postcss-autoreset": "^1.2.1",
    "postcss-discard-duplicates": "^2.1.0",
    "postcss-import": "^9.1.0",
    "postcss-initial": "^1.5.3",
    "postcss-loader": "^1.3.3",
    "postcss-scss": "^0.4.1",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "prop-types": "^15.5.8",
    "rc-slider": "^8.3.1",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.5.4",
    "react-transition-group": "^1.1.3",
    "remark-cli": "^3.0.0",
    "remark-lint": "^6.0.0",
    "remark-lint-code": "^2.0.0",
    "remark-lint-code-eslint": "^2.0.0",
    "remark-preset-lint-recommended": "^2.0.0",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "shelljs": "^0.7.7",
    "style-loader": "^0.16.1",
    "styled-components": "^2.2.1",
    "stylelint": "^7.10.1",
    "stylelint-declaration-strict-value": "^1.0.3",
    "stylelint-webpack-plugin": "^0.7.0",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^0.5.8",
    "watchify": "^3.9.0",
    "webpack": "2.2.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-stats-plugin": "^0.1.5",
    "whatwg-fetch": "^2.0.3"
  },
  "peerDependencies": {
    "react": "^15.5.4"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "space"
      ]
    ]
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/src/utils/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
