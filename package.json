{
  "name": "athenaeum",
  "version": "2.0.1",
  "description": "PolicyGenius React Component Library",
  "main": "lib/index.js",
  "scripts": {
    "prepush": "yarn run lint",
    "open": "open http://localhost:6060",
    "cc": "yarn run plop create-component",
    "build:publish": "./node_modules/webpack/bin/webpack.js --config ./webpack.config.publish.js --progress --colors",
    "build:publish:watch": "./node_modules/webpack/bin/webpack.js --config ./webpack.config.publish.js --progress --colors --watch",
    "publish": "yarn run build:publish && npm publish",
    "start": "yarn run styleguide:build && yarn run styleguide",
    "test": "jest",
    "stylin": "yarn run start  && yarn run open",
    "styleguide": "styleguidist server",
    "styleguide:build": "rm -rf ./styleguide/* && styleguidist build",
    "lint": "yarn run lint:js && yarn run lint:css && yarn run lint:md",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:md": "eslint --ext md ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/policygenius/athenaeum.git"
  },
  "author": "PolicyGenius",
  "license": "MIT",
  "homepage": "https://github.com/policygenius/athenaeum#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "lodash": "^4.17.4",
    "react-svg-inline": "^2.0.0",
    "sinon": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-markdown": "https://github.com/jmcolella/eslint-plugin-markdown/tarball/master",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.0",
    "html-webpack-plugin": "^2.24.1",
    "husky": "^0.13.3",
    "image-webpack-loader": "^3.3.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-verbose-reporter": "0.0.3",
    "karma-webpack": "^1.8.0",
    "klaw-sync": "^2.1.0",
    "markdownlint-cli": "^0.3.1",
    "mocha": "^3.1.2",
    "node-sass": "^4.5.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "plop": "^1.7.4",
    "postcss-autoreset": "^1.2.1",
    "postcss-discard-duplicates": "^2.1.0",
    "postcss-import": "^9.1.0",
    "postcss-initial": "^1.5.3",
    "postcss-loader": "^1.3.3",
    "postcss-scss": "^0.4.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "react-styleguidist": "^5.1.8",
    "react-test-renderer": "^15.5.4",
    "remark-cli": "^3.0.0",
    "remark-lint": "^6.0.0",
    "remark-lint-code": "^2.0.0",
    "remark-lint-code-eslint": "^2.0.0",
    "remark-preset-lint-recommended": "^2.0.0",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "shelljs": "^0.7.7",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-declaration-strict-value": "^1.0.3",
    "stylelint-webpack-plugin": "^0.7.0",
    "svg-inline-loader": "^0.7.1",
    "url-loader": "^0.5.8",
    "watchify": "^3.9.0",
    "webpack": "2.2.1",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.13.2"
  },
  "peerDependencies": {
    "react": "^15.5.4"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "space"
      ]
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "modulePaths": ["src"],
    "moduleDirectories": ["node_modules"],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/src/utils/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
